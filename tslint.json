{
  "extends": ["@1stg/tslint-config/angular", "@1stg/tslint-config/eslint"],
  "rules": {
//    "component-selector": [true, "element", "alo", "kebab-case"],
    "deprecation": {
      "severity": "warning"
    },
//    "directive-selector": [true, "attribute", "alo", "camelCase"],
